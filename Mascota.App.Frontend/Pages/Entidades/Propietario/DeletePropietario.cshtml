@page
@model Mascota.App.Frontend.Pages.DeletePropietarioModel
@{
}
@* <h2>¿Está seguro que desea eliminar el Propietario con el Id: @Model.Propietario.Id ?</h2> *@
<form method="post">
    <input type="hidden" asp-for="Propietario.Id" />
    <div class="form-group">
        <label asp-for="Propietario.Nombre"></label>
        <input asp-for="Propietario.Nombre" class="form-control" disabled>
        <span class="text-danger" asp-validation-for="Propietario.Nombre"></span>

    </div>
    <div class="form-group">
        <label asp-for="Propietario.Apellidos"></label>
        <input asp-for="Propietario.Apellidos" class="form-control" disabled>
        <span class="text-danger" asp-validation-for="Propietario.Apellidos"></span>

    </div>
    <div class="form-group">
        <label asp-for="Propietario.Identificacion"></label>
        <input asp-for="Propietario.Identificacion" class="form-control" disabled>
        <span class="text-danger" asp-validation-for="Propietario.Identificacion"></span>

    </div>
    <div class="form-group">
        <label asp-for="Propietario.Telefono"></label>
        <input asp-for="Propietario.Telefono" class="form-control" disabled>
        <span class="text-danger" asp-validation-for="Propietario.Telefono"></span>

    </div>
    <div class="form-group">
        <label asp-for="Propietario.Direccion"></label>
        <input asp-for="Propietario.Direccion" class="form-control" disabled>
        <span class="text-danger" asp-validation-for="Propietario.Direccion"></span>

    </div>

<button type="submit" class="btn btn-primary">Eliminar</button>
</form>
<br>

<a asp-page="./Propietario" class="btn-default">Lista de Propietarios</a>